<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <include layout="@layout/title_normal"/>


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <FrameLayout
            android:id="@+id/anim_mine_fl"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <include layout="@layout/about_school_item"/>
        </FrameLayout>

    </RelativeLayout>

    <FrameLayout
        android:id="@+id/tab_layout_fl"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <include layout="@layout/common_tab_layout"/>
    </FrameLayout>

    <RelativeLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">


    <RelativeLayout
        android:id="@+id/mine_fragment_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />



    </RelativeLayout>

</LinearLayout>